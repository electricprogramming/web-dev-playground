{
  "rewrites": [
    {
      "source": "/",
      "destination": "/src/webpages/home/index.html"
    },
    {
      "source": "/js",
      "destination": "/src/webpages/JavaScript/index.html"
    },
    {
      "source": "/css",
      "destination": "/src/webpages/CSS/index.html"
    },
    {
      "source": "/html",
      "destination": "/src/webpages/HTML/index.html"
    },
    {
      "source": "/svg",
      "destination": "/src/webpages/SVG/index.html"
    },
    {
      "source": "/preview",
      "destination": "/src/webpages/preview/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/src/webpages/404/index.html"
    }
  ]
}